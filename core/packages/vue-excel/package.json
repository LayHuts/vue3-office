{
  "name": "@vue3-office/excel",
  "version": "2.0.0",
  "description": "Vue3 excel文档预览组件",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./lib/index.css": "./lib/index.css"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "copyType": "cp index.d.ts lib/index.d.ts",
    "build": "vite build && pnpm copyType",
    "publish-npm": "npm publish --access public"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "lodash": "^4.17.21",
    "x-data-spreadsheet": "^1.1.9",
    "xlsx": "^0.18.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LayHuts/vue3-office.git"
  },
  "keywords": [
    "vue3",
    "excel",
    "excel-preview"
  ],
  "license": "MIT",
  "author": "LayHuts"
}
