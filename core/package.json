{
  "name": "vue-office",
  "version": "1.0.0",
  "description": "通过Vue3开发的办公文档预览组件，支持docx、pdf、ppt、excel的预览",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lib": "pnpm -r --filter './packages/*' run build",
    "lib:vue-excel": "pnpm --filter @vue-office/excel run build",
    "lib:vue-docx": "pnpm --filter @vue-office/docx run build",
    "lib:vue-pdf": "pnpm --filter @vue-office/pdf run build",
    "lib:vue-pptx": "pnpm --filter @vue-office/pptx run build"
  },
  "dependencies": {
    "ant-design-vue": "^4.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "docx-preview": "^0.3.2",
    "exceljs": "^4.3.0",
    "x-data-spreadsheet": "^1.1.9",
    "xlsx": "^0.18.5",
    "lodash": "^4.17.21",
    "tinycolor2": "^1.6.0",
    "pptx-preview": "^1.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.4.11",
    "less": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LayHuts/vue-office.git"
  },
  "keywords": [
    "vue",
    "docx",
    "pdf",
    "ppt",
    "excel",
    "docx-preview",
    "excel-preview",
    "pdf-preview"
  ],
  "license": "MIT",
  "author": "LayHuts"
}
